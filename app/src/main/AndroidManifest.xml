<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="buyerseller.cs646.sdsu.edu.sellit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CategoryActivity"
            android:label="Select Category "/>
        <activity android:name=".UserLoginActivity"
            android:label="Login"/>
        <activity android:name=".ItemActivity"
            android:label="Item Details"/>
        <activity android:name=".UserRegistrationActivity"
            android:label="Please Register" >
        </activity>
        <activity android:name=".CategoryItemSubItemActivity"
            android:label="CategoryItem SubItem" >
        </activity>
    </application>

</manifest>